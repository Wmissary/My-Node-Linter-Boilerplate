{
    "env": {
        "es2022": true,
        "node": true
    },
    "extends": [
        "eslint:recommended","plugin:unicorn/recommended","plugin:sonarjs/recommended","plugin:node/recommended","plugin:security/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["unicorn","sonarjs", "node", "security"],
    "rules": {
        "indent": ["error",2, { "SwitchCase": 1 }],
        "quotes": ["error", "double"],
        "semi": ["error", "always"]
    }
}
